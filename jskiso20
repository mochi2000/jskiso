           const locations = [
             {
               "name": "国会議事堂",
               "address": "東京都千代田区"
             },
             {
               "name": "札幌時計台",
               "address": "札幌市中央区北"
             },
             {
               "name": "首里城",
               "address": "沖縄"
             }
           ];

              for(let i=0; i<locations.length; i++) {
                document.write(locations[i].name + '/' +locations[i].address + '<br>');
              }
        </script>
